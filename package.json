{
	"name": "okineadev-website",
	"version": "0.0.0",
	"description": "A personal portfolio website showcasing my projects, blog posts, and contact information.",
	"author": "Okinea Dev",
	"homepage": "https://okinea.dev",
	"repository": {
		"type": "git",
		"url": "https://github.com/okineadev/okineadev-website.git"
	},
	"bugs": {
		"url": "https://github.com/okineadev/okineadev-website/issues"
	},
	"license": "All Rights Reserved.",
	"private": true,
	"type": "module",
	"scripts": {
		"vercel": "vercel",
		"build": "vue-tsc -b && vite-ssg build",
		"preview": "vite preview",
		"dev": "vite",
		"build:cv": "bun run --filter=./cv build && bun run --filter=./cv render:pdf",
		"build:all": "bun run build && bun run build:cv",
		"preview:cv": "bun run --filter=./cv preview",
		"dev:cv": "bun run --filter=./cv dev",
		"render:cli": "bun run --filter=./cli render",
		"lint": "biome lint",
		"format": "prettier **/*.{vue,scss,md,html,yml} -w && biome format --write",
		"svgo": "svgo src/assets/images/*.svg src/assets/icons/*.svg"
	},
	"workspaces": {
		"packages": ["cli", "cv"],
		"catalog": {
			"vue": "^3.5.19",
			"bun-types": "^1.2.20",
			"vite": "^7.1.3",
			"@vitejs/plugin-vue": "^6.0.1",
			"typescript": "^5.9.2"
		}
	},
	"dependencies": {
		"lenis": "^1.3.8",
		"vue": "catalog:"
	},
	"peerDependencies": {
		"react": "^18.3.1"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@iconify-json/logos": "^1.2.9",
		"@iconify-json/lucide": "^1.2.62",
		"@iconify-json/octicon": "^1.2.10",
		"@vitejs/plugin-vue": "catalog:",
		"@vue/compiler-sfc": "^3.5.19",
		"bun-types": "catalog:",
		"prettier": "^3.6.2",
		"sass-embedded": "^1.90.0",
		"schema-dts": "^1.1.5",
		"ts-node": "^10.9.2",
		"typescript": "catalog:",
		"unplugin-icons": "^22.2.0",
		"unplugin-vue-components": "^29.0.0",
		"vercel": "^45.0.9",
		"vite": "catalog:",
		"vite-plugin-html": "^3.2.2",
		"vite-plugin-open-graph": "^2.0.6",
		"vite-plugin-sitemap": "^0.8.2",
		"vite-ssg": "^28.0.0",
		"vue-tsc": "^3.0.6"
	},
	"overrides": {
		"ts-node": "^10.9.2",
		"typescript": "^5.8.2"
	},
	"optionalDependencies": {
		"svgo": "^4.0.0"
	},
	"trustedDependencies": ["@biomejs/biome", "@parcel/watcher", "@tailwindcss/oxide", "esbuild", "puppeteer"]
}
